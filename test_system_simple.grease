use system

print("Testing system module...")

# Test basic command execution
result = exec("echo", "Hello from system module!")
if result.success:
    print("exec() works: " + result.stdout.trim())
else:
    print("exec() failed: " + result.stderr)

# Test environment variables
home = getenv("HOME")
if home != null:
    print("getenv() works: HOME = " + home)
else:
    print("getenv() failed for HOME")

# Test setting environment variable
setenv("TEST_VAR", "test_value")
test_val = getenv("TEST_VAR")
if test_val == "test_value":
    print("setenv() works: TEST_VAR = " + test_val)
else:
    print("setenv() failed")

# Test shell command
shell_result = shell("echo 'Shell test'")
if shell_result.success:
    print("shell() works: " + shell_result.stdout.trim())
else:
    print("shell() failed: " + shell_result.stderr)

print("System module test completed!")