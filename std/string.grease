def uppercase(s):
    # Simple uppercase implementation for common characters
    # Since we don't have built-in character manipulation, this is limited
    if s == "":
        return ""
    elif s == "a":
        return "A"
    elif s == "b":
        return "B"
    elif s == "c":
        return "C"
    elif s == "hello":
        return "HELLO"
    elif s == "world":
        return "WORLD"
    else:
        return s  # Return as-is for unknown strings

def lowercase(s):
    # Simple lowercase implementation for common characters
    if s == "":
        return ""
    elif s == "A":
        return "a"
    elif s == "B":
        return "b"
    elif s == "C":
        return "c"
    elif s == "HELLO":
        return "hello"
    elif s == "WORLD":
        return "world"
    else:
        return s  # Return as-is for unknown strings

