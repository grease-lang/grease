class Animal:
    fn make_sound():
        print("Some generic animal sound")

class Dog(Animal):
    fn make_sound():
        print("Woof!")
    
    fn fetch():
        print("Fetching the ball!")

# Test basic class functionality
dog = new Dog()
dog.make_sound()
dog.fetch()

# Test property access and assignment
dog.name = "Buddy"
dog.age = 3
print("Dog name:", dog.name)
print("Dog age:", dog.age)

# Test inheritance
animal = new Animal()
animal.make_sound()

# Test method with parameters
class Calculator:
    fn add(a, b):
        return a + b
    
    fn multiply(a, b):
        return a * b

calc = new Calculator()
result1 = calc.add(5, 3)
result2 = calc.multiply(4, 7)
print("5 + 3 =", result1)
print("4 * 7 =", result2)

# Test super() calls
class Cat(Animal):
    fn make_sound():
        print("Meow!")
    
    fn make_parent_sound():
        super.make_sound()

cat = new Cat()
cat.make_sound()
cat.make_parent_sound()