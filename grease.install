[Package]
name=grease
version=0.1.1
section=interpreters
priority=optional
architecture=amd64
depends=libc6
maintainer=Grease Developers <dev@grease-lang.org>
description=Grease Scripting Language
 A modern scripting language written in pure Rust.

[Install]
mkdir -p /usr/local/bin
cp target/release/grease /usr/local/bin/
chmod +x /usr/local/bin/grease