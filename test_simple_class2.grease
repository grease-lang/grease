class Animal:
    fn make_sound():
        print("Some generic animal sound")

class Dog(Animal):
    fn make_sound():
        print("Woof!")
    
    fn fetch():
        print("Fetching the ball!")

# Test basic class functionality
dog = new Dog()
dog.make_sound()
dog.fetch()

# Test property access and assignment
dog.name = "Buddy"
dog.age = 3
print("Dog name:", dog.name)
print("Dog age:", dog.age)

# Test inheritance
animal = new Animal()
animal.make_sound()